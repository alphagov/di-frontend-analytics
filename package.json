{
  "name": "di-frontend-analytics",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "bundle-js-analytics": "echo \"bundling analytics JavaScript\" && mkdir -p dist/assets/javascript && uglifyjs src/assets/javascript/analytics/**/*.js src/assets/javascript/analytics/*.js -o dist/assets/javascript/analytics.js",
    "test:unit": "mocha -p --recursive \"test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/di-frontend-analytics.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphagov/di-frontend-analytics/issues"
  },
  "homepage": "https://github.com/alphagov/di-frontend-analytics#readme",
  "devDependencies": {
    "chai": "4.3.7",
    "jsdom": "22.1.0",
    "mocha": "10.2.0",
    "prettier": "3.0.2",
    "sinon": "15.2.0",
    "sinon-chai": "3.7.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "uglify-js": "3.15.1"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js",
      "ts"
    ],
    "timeout": 20000,
    "recursive": true,
    "require": [
      "ts-node/register"
    ],
    "exit": true
  }
}
